{
  "name": "corider",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "Co-Rider Mobile App - Affordable ride sharing platform",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "keywords": [
    "ride-sharing",
    "transportation",
    "mobile-app",
    "react-native",
    "expo",
    "typescript"
  ],
  "author": "Wellborn Michael",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/corider.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/corider/issues"
  },
  "homepage": "https://corider.com",
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
